{
  "name": "CLMTools",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "start": "node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "d-serve": "docker-compose run --rm --service-ports node npm run serve",
    "d-install": "docker-compose run --rm node npm install",
    "d-lint": "docker-compose run --rm node npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "cropperjs": "^1.5.13",
    "express": "^4.18.1",
    "inputmask": "^5.0.7",
    "laravel-echo": "^1.13.1",
    "lodash": "^4.17.21",
    "pusher-js": "^7.4.0",
    "v-emoji-picker": "^2.3.3",
    "vue": "^2.6.12",
    "vue-i18n": "^8.27.2",
    "vue-lodash": "^2.1.2",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "postinstall": "npm run build",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "prettier/prettier": [
        "warn",
        {
          "tabWidth": 2,
          "singleQuote": true,
          "semi": true,
          "trailingComma": "none"
        }
      ],
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase",
        {
          "registeredComponentsOnly": false
        }
      ],
      "vue/eqeqeq": "error",
      "semi": "error",
      "no-console": "warn",
      "no-debugger": "warn",
      "eqeqeq": "error",
      "curly": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
